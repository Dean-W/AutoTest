[
  {
    "description": "这是一个获取cookies信息的get请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "恭喜你获取cookies信息"

    }
  },
{
    "description":"模拟带cookies的get请求",
    "request": {
      "uri": "/getwithcookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个携带cookies信息才能访问的请求"

    }

  },
  {
    "description": "这是一个带cookies的post请求",
    "request": {
      "uri": "/postwithcookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age":"18"
      }
    },
    "response": {
      "status": 200,
      "json":{
        "huhansan":"success",
        "status": "1"
      }
    }
  }
]